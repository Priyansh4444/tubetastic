{
    "manifest_version": 3,
    "name": "YT (Name)",
    "description": "YouTube video analyzer",
    "version": "0.0.1",
    "icons": {
    },
    "action": {
        "default_title": "",
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "https://*.youtube.com/*"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
    },
    "content_scripts": [{
        "js": ["dist/foreground.js"],
        "matches": ["https://youtube.com/*"]
    }]
}
